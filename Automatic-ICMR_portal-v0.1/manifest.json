{
  "name":"J Plus G Automatic Tool",
  "description": "this extension say some infomation state ,taluk, pincode and area",
  "version":"1.0.1",
  "manifest_version":2,
  "author":"Joel T George",
  "permissions": [
    "activeTab",
    "storage",
    "<all_urls>"

  ],    
  "browser_action":{
    "default_icon":"images/logo.png",  
    "default_popup":"popup.html",
    "js":["libs/jquery-3.6.0.js","popup.js"]
  },   
  "icons":{
      "16":"images/logo16px.png",
      "32":"images/logo32px.png",
      "48":"images/logo48px.png",
      "128":"images/logo128px.png"
  },
  "background":{
    "scripts": ["background.js"],
    "persistent": false
  },
   "content_scripts": [
   {
     "matches": ["<all_urls>"],
     
     "js": ["content.js"]
   }
 ]

  
}